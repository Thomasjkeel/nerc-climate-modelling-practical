     "output_type": "display_data"
    }
   ],
   "source": [
    "# quick plot of the temperature anomaly\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "years = t + 1850\n",
    "\n",
    "plt.plot(years,temp_anom)\n",
    "plt.xlabel('Year')\n",
    "plt.xlim(1850,2025)\n",
    "plt.ylabel('Temperature Anomaly (K)')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Now make a model that fits function for upper ocean temperature to the temperature anomaly data\n",
    "# in order to find parameters for function that give the best fit using least squares approach\n",
    "\n",
    "from lmfit import Model\n",
    "\n",
    "mod = Model(T)\n",
    "params = mod.make_params(A=-1,B=-1,F=2,alpha=1)\n",
    "\n",
    "# need to constrain the parameters as A + B = -F/alpha\n",
    "params['F'].expr = '-alpha*(A+B)'\n",
    "\n",
    "# Radiative forcing must be positive\n",
    "params['F'].min = 0\n",
    "# Alpha is 1.04+-0.36 from CMIP6 (in slides)\n",
    "params['alpha'].min = 0.68\n",
    "params['alpha'].max = 1.40\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x432 with 2 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "result = mod.fit(temp_anom, params, t=t, method='least_squares')\n",
    "\n",
    "result.plot()\n",
    "plt.show()"
   ]
  },
